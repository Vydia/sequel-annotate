#!/usr/bin/env ruby
require 'rubygems'
begin
  require 'bundler'
  Bundler.setup
rescue Exception => e
end

here = File.expand_path(File.dirname __FILE__)
$:<< "#{here}/../lib"

target = {
  :klass => AnnotateSequel,
  :task => :do_annotations,
}

Annotate.eager_load(options)
target[:klass].send(target[:task])
